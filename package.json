{
  "name": "vs-code-console-log-extension",
  "displayName": "vs-code-console-log-extension",
  "description": "Adds console.log() Using Keyboard Shortcut",
  "version": "0.0.3",
  "engines": {
    "vscode": ">=1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-code-console-log-extension.addConsoleLog",
        "title": "Add console.log()"
      }
    ],
    "keybindings": [
      {
        "command": "vs-code-console-log-extension.addConsoleLog",
        "key": "ctrl+shift+j",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "icon": "icon.png",
  "publisher": "umair-ali-bhutto",
  "repository": {
    "type": "git",
    "url": "https://github.com/umair-ali-bhutto/vs-code-console-log-extension.git"
  }
}