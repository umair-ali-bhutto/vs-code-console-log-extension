{
  "name": "vs-code-console-log-extension",
  "displayName": "vs-code-console-log-extension",
  "description": "Adds console.log() Using Keyboard Shortcut",
  "version": "0.0.4",
  "engines": {
    "vscode": ">=1.80.0"
  },
  "homepage": "https://github.com/umair-ali-bhutto/vs-code-console-log-extension",
  "keywords": [
    "console.log",
    "logging",
    "debugging",
    "javascript",
    "typescript",
    "developer tools",
    "productivity"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "contributors": [
    {
      "name": "Umair Ali Bhutto",
      "githubUsername": "umair-ali-bhutto",
      "url": "https://github.com/umair-ali-bhutto"
    }
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-code-console-log-extension.addConsoleLog",
        "title": "Add console.log()"
      }
    ],
    "keybindings": [
      {
        "command": "vs-code-console-log-extension.addConsoleLog",
        "key": "ctrl+shift+j",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "icon": "assets/icon.png",
  "publisher": "umair-ali-bhutto",
  "repository": {
    "type": "git",
    "url": "https://github.com/umair-ali-bhutto/vs-code-console-log-extension.git"
  },
  "bugs": {
    "url": "https://github.com/umair-ali-bhutto/vs-code-console-log-extension/issues"
  },
  "badges": [
    {
      "url": "https://raster.shields.io/visual-studio-marketplace/i/umair-ali-bhutto.vs-code-console-log-extension",
      "href": "https://marketplace.visualstudio.com/items?itemName=umair-ali-bhutto.vs-code-console-log-extension",
      "description": "Visual Studio Installs Badge"
    }
  ],
  "galleryBanner": {
    "color": "#0f0f0f",
    "theme": "dark"
  }
}